{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(let|component|style)\\b.*\\{?\\s*$|^\\s*\\w+\\s*\\{\\s*$",
    "decreaseIndentPattern": "^\\s*\\}\\s*$|^\\s*(in|then|else)\\b.*$"
  }
}